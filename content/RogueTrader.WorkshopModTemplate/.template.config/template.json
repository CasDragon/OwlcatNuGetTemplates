{
	"$schema": "http://json.schemastore.org/template",
	"author": "ADDB",
	"classifications": [
		"UnityModManager",
		"UMM",
		"Mod",
		"Rogue Trader Mod",
		"Steam Workshop"
	],
	"identity": "RogueTraderWorkshopMod",
	"name": "UnityModManager Rogue Trader Workshop Mod",
	"shortName": "rtmodworkshop",
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"preferNameDirectory": true,
	"sourceName": "RogueTrader.WorkshopModTemplate",
	"symbols": {
		"TargetFramework": {
			"type": "parameter",
			"description": "The target framework for the project",
			"datatype": "text",
			"defaultValue": "net472",
			"replaces": "net472"
		},
		"Description": {
			"type": "parameter",
			"description": "Mod description",
			"datatype": "text",
			"replaces": "Template Mod",
			"defaultValue": "Template Mod"
		},
		"UnityVersion": {
			"type": "parameter",
			"description": "Unity version to use when developing the mod",
			"datatype": "text",
			"replaces": "2022.3.7",
			"defaultValue": "2022.3.7"
		},
		"Version": {
			"type": "parameter",
			"description": "Mod version",
			"datatype": "text",
			"defaultValue": "1.0.0"
		},
		"VersionImpl": {
			"type": "generated",
			"generator": "join",
			"replaces": "<Version>1.0.0</Version>",
			"parameters": {
				"symbols": [
					{
						"type": "const",
						"value": "<Version>"
					},
					{
						"type": "ref",
						"value": "Version"
					},
					{
						"type": "const",
						"value": "</Version>"
					}
				]
			}
		}
	}
}